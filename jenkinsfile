pipeline{
	agent any
		stages{
			stage('One'){
				steps{
					echo 'Hi, this is Pavan from capgemini'
					}
				}
			stage('Two'){
				steps{
					input('Do you wnat to proceed?')
					}
				}
			stage('Three'){
				when{
					not{
						branch "master"
					}
					}
					steps{
						echo 'Hello'
						}
					}
				}
	}
